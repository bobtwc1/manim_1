%%manim -qm CircleToSquare

class CircleToSquare(Scene):
    def construct(self):
        #plane
        ax=NumberPlane().add_coordinates()
        ##basis points
        O=Dot(ax.c2p(0, 0))
        O_l=MathTex("O").next_to(ax.c2p(0,0)).scale(0.7).shift(0.2*DOWN+0.6*LEFT)
        I_l=MathTex("I").next_to(ax.c2p(1,0)).scale(0.7).shift(0.2*UP+0.4*LEFT)
        J_l=MathTex("J").next_to(ax.c2p(0,1)).scale(0.7).shift(0.1*DOWN)
        pla=VGroup(ax,O)
        plan_l=VGroup(O_l,J_l,I_l)
        self.play(Create(pla))
        self.play(Write(plan_l))
        self.wait(2)
        ## the two vectors u and v 
        u=Vector([4,0.5],color=GREEN)
        v=Vector([1.5,2.5],color=RED)
        u_l=MathTex('\overrightarrow{u}',color=GREEN).next_to(u).scale(1.5).shift(0.2*UP)
        v_l=MathTex('\overrightarrow{v}',color=RED).next_to(v).shift(1.3*UP+LEFT).scale(1.5)
        res=MathTex(r'det(','\overrightarrow{u}',',','\overrightarrow{v}',')=7.75').scale(1.3).to_edge(DOWN+LEFT)
        res[1].set_color(GREEN)
        res[3].set_color(RED)
        ## parallelogram and his surface
        pol=Polygon(ax.c2p(0,0),ax.c2p(4,0.5),ax.c2p(5.5,3),ax.c2p(1.5,2.5),color=YELLOW,fill_opacity=.3)
        #order matters
        order=VGroup(pol,u,v,u_l,v_l,res)
        self.play(Create(order))
        self.wait(1)
        self.play(Indicate(res))
        self.wait(2)
        self.play(FadeOut(u_l,v_l))
        self.wait(2)
        ## determinat first transform
        det1=VGroup(pol,v,u)
        v_2=Vector([-3,2],color=RED)
        pol1=Polygon(ax.c2p(0,0),ax.c2p(4,0.5),ax.c2p(1,2.5),ax.c2p(-3,2),color=YELLOW,fill_opacity=.3)
        det2=VGroup(pol1,v_2,u)
        self.play(det1.animate.become(det2))
        self.wait(1)
        res1=MathTex(r'det(','\overrightarrow{u}',',','\overrightarrow{v}',')=12.5').scale(1.3).to_edge(DOWN+LEFT)
        res1[1].set_color(GREEN)
        res1[3].set_color(RED)
        self.play(res.animate.become(res1))
        self.play(Indicate(res))
        self.wait(2)

        ## determinat second transform
        
        v_3=Vector([-3,-2],color=RED)
        u_3=Vector([-3,1],color=GREEN)
        uc=u_3.copy().shift(DOWN*2+LEFT*3)
        pol2=Polygon(ax.c2p(0,0),ax.c2p(-3,1),ax.c2p(-6,-1),ax.c2p(-3,-2),color=YELLOW,fill_opacity=.3)
        det3=VGroup(pol2,v_3,u_3)
        self.play(det1.animate.become(det3))
        self.wait(1)
        res2=MathTex(r'det(','\overrightarrow{u}',',','\overrightarrow{v}',')=9').scale(1.3).to_edge(DOWN+LEFT)
        res2[1].set_color(GREEN)
        res2[3].set_color(RED)
        self.play(res.animate.become(res2))
        self.play(Indicate(res))
        self.wait(2)
        
        ## determinat second transform
        
        v_4=Vector([-3,0],color=RED)
        uc=u_3.copy().shift(LEFT*3)
        pol3=Polygon(ax.c2p(0,0),ax.c2p(-3,1),ax.c2p(-6,1),ax.c2p(-3,0),color=YELLOW,fill_opacity=.3)
        det4=VGroup(pol3,v_4,u_3)
        self.play(det1.animate.become(det4))
        self.wait(1)
        res3=MathTex(r'det(','\overrightarrow{u}',',','\overrightarrow{v}',')=3').scale(1.3).to_edge(DOWN+LEFT)
        res3[1].set_color(GREEN)
        res3[3].set_color(RED)
        self.play(res.animate.become(res3))
        self.play(Indicate(res))
        self.wait(2)
        ## determinat second transform
        
        v_5=Vector([3,-1],color=RED)
        uc=u_3.copy().shift(LEFT*3)
        pol4=Polygon(ax.c2p(0,0),ax.c2p(-3,1),ax.c2p(0,0),ax.c2p(3,-1),color=YELLOW,fill_opacity=.3)
        det4=VGroup(pol4,v_5,u_3)
        self.play(det1.animate.become(det4))
        self.wait(1)
        res4=MathTex(r'det(','\overrightarrow{u}',',','\overrightarrow{v}',')=0').scale(1.3).to_edge(DOWN+LEFT)
        res4[1].set_color(GREEN)
        res4[3].set_color(RED)
        self.play(res.animate.become(res4))
        self.play(Indicate(res))
        self.wait(2)
        
        self.play(FadeOut(pol))
        #begin null determinant
        k=ValueTracker(1)
        v.add_updater(lambda m: m.become(Vector([3*k.get_value(),-1*k.get_value()],color=RED)))
        self.play(k.animate.set_value(1.5))
        self.wait(1)
        res5=MathTex(r'det(','\overrightarrow{u}',',','\overrightarrow{v}',')=0').scale(1.3).to_edge(DOWN+LEFT)
        res5[1].set_color(GREEN)
        res5[3].set_color(RED)
        self.play(res.animate.become(res5))
        self.play(Indicate(res))
        self.wait(2)
        
        self.play(k.animate.set_value(2))
        self.play(Indicate(res))
        self.wait(2)
        self.play(k.animate.set_value(0.5))
        self.play(Indicate(res))
        self.wait(2)
        self.play(k.animate.set_value(-0.5))
        self.play(Indicate(res))
        self.wait(2)
        self.play(k.animate.set_value(-1.2))
        self.play(Indicate(res))
        self.wait(2)
        self.play(k.animate.set_value(-2))
        self.play(Indicate(res))
        self.wait(2)

